#include "project_cpp.h"
#include "W25Qx_module.h"

SPI_HandleTypeDef hspi1;

W25Q64 myFlash(&hspi1, GPIOF, GPIO_PIN_13);

void main_cpp(void)
{
	if (myFlash.init() == HAL_ERROR)
	{
		while(1);
	}
	while(1)
	{
		myFlash.read_JDEC();
		/*myFlash.write_enable();
		HAL_Delay(250);
		myFlash.read_status_register();
		HAL_Delay(250);
		myFlash.write_disable();
		HAL_Delay(250);
		myFlash.read_status_register();
		HAL_Delay(250);*/
	}
}
