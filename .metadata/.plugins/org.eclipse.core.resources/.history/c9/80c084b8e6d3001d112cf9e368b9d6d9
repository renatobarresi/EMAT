#include "project_cpp.h"
#include "spi_module.h"

SPI_HandleTypeDef hspi1;

class SPI mySPI(&hspi1, GPIO_PIN_13, GPIOF);

void main_cpp(void)
{
	if (mySPI.init() == HAL_ERROR)
	{
		while(1);
	}
	mySPI.csLow();
	while(1)
	{
	}
}
