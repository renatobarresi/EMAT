#include "W25Qx_module.h"

/*Constructor*/
W25Q64::W25Q64(SPI_HandleTypeDef *hspi, GPIO_TypeDef *cs_gpio_port, uint16_t cs_pin)
	: mySPIPort(this->hspi, this->csPin, this->csGPIOPort);
{
	this->hspi = hspi;
	this->csPin = cs_pin;
	this->csGPIOPort = cs_gpio_port;
}

